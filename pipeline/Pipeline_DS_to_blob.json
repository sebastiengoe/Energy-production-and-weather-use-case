{
	"name": "Pipeline_DS_to_blob",
	"properties": {
		"activities": [
			{
				"name": "Get_current_weather",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "JsonSource",
						"storeSettings": {
							"type": "AzureBlobStorageReadSettings",
							"recursive": true,
							"enablePartitionDiscovery": false
						},
						"formatSettings": {
							"type": "JsonReadSettings"
						}
					},
					"sink": {
						"type": "SqlDWSink",
						"allowCopyCommand": true
					},
					"enableStaging": true,
					"stagingSettings": {
						"linkedServiceName": {
							"referenceName": "weatherAPI",
							"type": "LinkedServiceReference"
						},
						"path": "weatherrawdata/output"
					},
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"source": {
									"path": "$['location']['name']"
								},
								"sink": {
									"name": "LocationName",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "$['location']['region']"
								},
								"sink": {
									"name": "Region",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "$['location']['country']"
								},
								"sink": {
									"name": "Country",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "$['location']['lat']"
								},
								"sink": {
									"name": "Latitude",
									"type": "Decimal"
								}
							},
							{
								"source": {
									"path": "$['location']['lon']"
								},
								"sink": {
									"name": "Longitude",
									"type": "Decimal"
								}
							},
							{
								"source": {
									"path": "$['location']['localtime']"
								},
								"sink": {
									"name": "LocalTime",
									"type": "DateTime"
								}
							},
							{
								"source": {
									"path": "$['current']['last_updated']"
								},
								"sink": {
									"name": "LastUpdated",
									"type": "DateTime"
								}
							},
							{
								"source": {
									"path": "$['current']['temp_c']"
								},
								"sink": {
									"name": "TemperatureC",
									"type": "Decimal"
								}
							},
							{
								"source": {
									"path": "$['current']['temp_f']"
								},
								"sink": {
									"name": "TemperatureF",
									"type": "Decimal"
								}
							},
							{
								"source": {
									"path": "$['current']['is_day']"
								},
								"sink": {
									"name": "IsDay",
									"type": "Boolean"
								}
							},
							{
								"source": {
									"path": "$['current']['condition']['text']"
								},
								"sink": {
									"name": "ConditionText",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "$['current']['condition']['icon']"
								},
								"sink": {
									"name": "ConditionIcon",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "$['current']['wind_mph']"
								},
								"sink": {
									"name": "WindMph",
									"type": "Decimal"
								}
							},
							{
								"source": {
									"path": "$['current']['wind_kph']"
								},
								"sink": {
									"name": "WindKph",
									"type": "Decimal"
								}
							},
							{
								"source": {
									"path": "$['current']['wind_degree']"
								},
								"sink": {
									"name": "WindDegree",
									"type": "Int32"
								}
							},
							{
								"source": {
									"path": "$['current']['wind_dir']"
								},
								"sink": {
									"name": "WindDirection",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "$['current']['pressure_mb']"
								},
								"sink": {
									"name": "PressureMb",
									"type": "Decimal"
								}
							},
							{
								"source": {
									"path": "$['current']['pressure_in']"
								},
								"sink": {
									"name": "PressureIn",
									"type": "Decimal"
								}
							},
							{
								"source": {
									"path": "$['current']['precip_mm']"
								},
								"sink": {
									"name": "PrecipMm",
									"type": "Decimal"
								}
							},
							{
								"source": {
									"path": "$['current']['precip_in']"
								},
								"sink": {
									"name": "PrecipIn",
									"type": "Decimal"
								}
							},
							{
								"source": {
									"path": "$['current']['humidity']"
								},
								"sink": {
									"name": "Humidity",
									"type": "Int32"
								}
							},
							{
								"source": {
									"path": "$['current']['cloud']"
								},
								"sink": {
									"name": "Cloud",
									"type": "Int32"
								}
							},
							{
								"source": {
									"path": "$['current']['feelslike_c']"
								},
								"sink": {
									"name": "FeelsLikeC",
									"type": "Decimal"
								}
							},
							{
								"source": {
									"path": "$['current']['feelslike_f']"
								},
								"sink": {
									"name": "FeelsLikeF",
									"type": "Decimal"
								}
							},
							{
								"source": {
									"path": "$['current']['vis_km']"
								},
								"sink": {
									"name": "VisibilityKm",
									"type": "Decimal"
								}
							},
							{
								"source": {
									"path": "$['current']['vis_miles']"
								},
								"sink": {
									"name": "VisibilityMiles",
									"type": "Decimal"
								}
							},
							{
								"source": {
									"path": "$['current']['uv']"
								},
								"sink": {
									"name": "UvIndex",
									"type": "Decimal"
								}
							},
							{
								"source": {
									"path": "$['current']['gust_mph']"
								},
								"sink": {
									"name": "GustMph",
									"type": "Decimal"
								}
							},
							{
								"source": {
									"path": "$['current']['gust_kph']"
								},
								"sink": {
									"name": "GustKph",
									"type": "Decimal"
								}
							}
						],
						"mapComplexValuesToString": true
					}
				},
				"inputs": [
					{
						"referenceName": "current_weather_blob",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "current_weather_api",
						"type": "DatasetReference"
					}
				]
			}
		],
		"annotations": []
	}
}